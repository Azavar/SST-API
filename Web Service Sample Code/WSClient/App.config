<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>

  <appSettings>
    <add key="SSTUser" value="{state user name}"/>
    <add key="SSTPass" value="{password}"/>
  </appSettings>

  <system.serviceModel>
    <bindings>
      <wsHttpBinding>
        <binding name="WSHttpBinding_IApiService" maxReceivedMessageSize="2147483647">
          <security>
            <message clientCredentialType="UserName" 
                     negotiateServiceCredential="false" 
                     establishSecurityContext="false" />
          </security>
        </binding>
      </wsHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://test.sstregister.org/Api.svc" binding="wsHttpBinding"
        bindingConfiguration="WSHttpBinding_IApiService" contract="ServiceReference1.IApiService"
        name="WSHttpBinding_IApiService">
        <identity>
          <certificate encodedValue="" />
        </identity>
      </endpoint>
    </client>
  </system.serviceModel>
</configuration>


