						<xs:element name="ForeignPaymentAmount" type="ACHAmountType">
							<xs:annotation>
								<xs:documentation/>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="PaymentDetail">
				<xs:complexType>
					<xs:sequence>
