			<xs:element name="ModelTwo" type="ModelTwoType"/>
			<xs:element name="ModelThree"/>
			<xs:element name="None"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="ModelOneType">
		<xs:attribute name="CSPCode" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="ModelTwoType">
		<xs:attribute name="CASCode" type="xs:string" use="required"/>
