		</xs:restriction>
	</xs:simpleType>
	<!-- ACH Name Type -->
	<xs:simpleType name="ACHNameType">
		<xs:restriction base="xs:string">
			<xs:maxLength value="22"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- FTA Tax Type Code Type-->
	<xs:complexType name="FTATaxTypeCode">
		<xs:sequence>
			<xs:element name="FTACode">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="[0-9]{3}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="StateTaxTypeCode">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="[0-9]{2}|[  ]{2}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--ACH Entity Detail Type Used for ACH Debit-->
	<xs:complexType name="ACHEntityDetailType">
		<xs:sequence>
			<xs:choice>
				<xs:element name="Checking" type="CheckboxType">
					<xs:annotation>
						<xs:documentation>
							<Description>Checking Account</Description>
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Savings" type="CheckboxType">
					<xs:annotation>
						<xs:documentation>
							<Description>Savings Account</Description>
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="RoutingTransitNumber" type="RoutingTransitNumberType"/>
			<xs:element name="BankAccountNumber" type="BankAccountNumberType"/>
			<xs:element name="PaymentAmount" type="ACHAmountType"/>
			<xs:element name="IdentificationNumber" type="ACHIdentifierType" minOccurs="0"/>
			<xs:element name="AccountHolderName" type="ACHNameType" minOccurs="0"/>
			<xs:element name="AccountHolderType" type="BankAccountHolderType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>"1" for Business or "2" for Personal</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RequestedPaymentDate" type="DateType" minOccurs="0"/>
			<xs:element name="AddendaRecord" type="AddendaRecordType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!--ACH Credit Detail Type used for ACH Credit -->
	<xs:complexType name="ACHCreditDetailType">
		<xs:sequence>
